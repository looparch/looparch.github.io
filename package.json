    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-glamor": "^1.6.13",
    "gatsby-plugin-google-tagmanager": "^1.0.19",
    "gatsby-plugin-manifest": "^1.0.27",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-offline": "^1.0.18",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-plugin-sitemap": "^1.2.25",
    "gatsby-source-contentful": "^1.3.38",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.32",
    "gatsby-transformer-sharp": "^1.6.27",
    "inquirer": "^5.1.0",
